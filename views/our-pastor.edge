<script>
$('head').prepend('<link rel="stylesheet" href="../cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.2.0/assets/owl.carousel.css">');

$(function() {
	var sliderTriggers = $('.Other-Ministry__logos .Other-Ministry__logos--box');
	var ministrySlider = $('.Other-Ministry__slider-set');
	ministrySlider.owlCarousel({
		autoPlay: 3000,
		items: 1,
		nav: true,
		navText: [
		'<div class="carousel-nav left">PREVIOUS</div>',
		'<div class="carousel-nav right">NEXT</div>'
		]
	});
	ministrySlider.on('changed.owl.carousel', function(e) {
		sliderTriggers.removeClass('active');
		$(sliderTriggers[e.item.index]).addClass('active');
	});
	sliderTriggers.on('click', function(e) {
		var i = $.inArray(e.currentTarget, sliderTriggers);
		ministrySlider.trigger('to.owl.carousel', [i, 100]);
	});
})
</script>


