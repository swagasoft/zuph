$(document).ready( function(){
	$('body').addClass('Index-Page');
		// $('a').click(function() {
		// 	return false;
		// });

var VIDE = $('.Video-Box').vide({
	mp4:'https://worldchangers.blob.core.windows.net/videoassets/wcci_promo.mp4',
	ogv:'https://worldchangers.blob.core.windows.net/videoassets/wcci_promo.ogv',
	webm:'https://worldchangers.blob.core.windows.net/videoassets/wcci_promo.webm',
	poster:'https://worldchangers.blob.core.windows.net/videoassets/wcci_promo.jpg'
}, {
	muted: true,
	loop: true,
	autoplay: true,
	position: '0 50%',
	posterType: 'jpg',
	resizing: true
});

});
$(document).ready(function() {

	var $window = $(window);
	var $vb = $('.Video-Box');
	var $vbi = $('.Video-Box__inner');
	var $vbii = $('.Video-Box__inner img');
	var $mnb = $('.MainNav__Bar');
	var $vbob = $('.Video-Box__overlay-bar');

	$vb.css('min-height', '500px');
	$vbii.height('auto');
	$vbob.css('min-height', '94px');

	function setVideoHeight() {

		if($window.innerWidth() > 768) {

			var h_vb = $window.height() - $mnb.innerHeight();
			$vb.height(h_vb);

			var h_vbi = $vb.innerHeight() - $vbob.innerHeight();
			$vbi.height(h_vbi);

		}
		else {
			$vb.height('auto');
			$vbi.height('auto');
		}

		var instance = $('.Video-Box').data('vide');
		instance.resize();

	}
	setVideoHeight();
	$(window).on('resize', setVideoHeight);

});
$(function() {
	var ehgs = [];
	$('[data-equalheight-group]').each(function () {
		var $this = $(this);
		var g = $this.data('equalheight-group');
		if(ehgs.indexOf(g) < 0)
			ehgs.push(g);
	});

	function doEqualHeights () {

		$(ehgs).each(function(index, groupName) {
			var items = $('[data-equalheight-group="' + groupName + '"]');
			var h = 0;
			$(items).css('minHeight',h);

			items.each(function(j, i) {
				var item = $(i);
				if(item.innerHeight() > h) h = item.innerHeight() + 1;
			});

			$(items).css('minHeight', h);
		})

	}

  window.onresize = function() {
  	clearTimeout(this.rsz);
  	this.rsz = setTimeout(doEqualHeights, 100);
  };

  doEqualHeights();
  $(window).load(doEqualHeights);
});
</script>

